val := 50;
zero_count := 0;
for (line <- read() split "\n" filter len) (
  count := line[1:].int;
  switch (line[0])
  case "L" -> (
    for (i <- 0 til count) (
      val = (val + 1) %% 100;
      if (val == 0) zero_count += 1;
     );
  )
  case "R" -> (
    for (i <- 0 til count) (
      val = (val + 1) %% 100;
      if (val == 0) zero_count += 1;
    )
  );
);
zero_count